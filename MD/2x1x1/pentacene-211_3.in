&CONTROL
  title = '# pentacene - C22 H14'
  pseudo_dir = './'
  calculation = 'md'
  nonadiabatic = .true.
  nstep = 2200
  dt = 20

/
 
&SYSTEM
  nat= 36
  ntyp= 2
  ibrav= 0
  ecutwfc = 40,
  ecutrho = 400
  
  occupations =  'smearing',
  smearing    =  'gaussian',
  degauss     =  0.03,
  vdw_corr = 'dft-d2',

    fde_kin_funct = 'revAPBEK'
    fde_split_mix = .true.
    fde_cell_split(1) = 0.8
    fde_cell_split(2) = 1
    fde_cell_split(3) = 1
    fde_nba_beg = 46 !homo=51
    fde_nba_end = 57 !lumo=52
     nosym = .true.
/
 
&ELECTRONS
    diagonalization='david'
    mixing_mode = 'plain'
    mixing_beta = 0.7
    conv_thr =  1.0d-9
    electron_maxstep =500
    use_gaussians = .true.

/

/
 
&IONS
  ion_dynamics = 'verlet'
  ion_temperature = 'rescaling'
  tempw = 300

/
 
