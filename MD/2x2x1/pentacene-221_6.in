  ntyp= 2
  ibrav= 0
  ecutwfc = 40,
  ecutrho = 400
  nbnd = 65
  occupations =  'smearing',
  smearing    =  'gaussian',
  degauss     =  0.03,
  vdw_corr = 'dft-d2',
